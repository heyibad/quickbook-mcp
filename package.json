{
    "name": "@qboapi/qbo-mcp-server",
    "version": "0.0.1",
    "description": "MCP server implementation for QuickBooks Online integration",
    "license": "MIT",
    "main": "dist/src/index.js",
    "type": "module",
    "bin": "./dist/src/index.js",
    "files": [
        "dist"
    ],
    "engines": {
        "node": ">=18.x",
        "npm": ">=9.x"
    },
    "scripts": {
        "build": "tsc && shx chmod +x dist/src/*.js",
        "prepare": "npm run build",
        "postinstall": "npm run build",
        "watch": "tsc --watch",
        "lint:fix": "eslint --fix",
        "lint": "eslint .",
        "auth": "node dist/src/auth-server.js",
        "start": "node dist/src/index.js",
        "test": "npm run build && node run-tests.js"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.20.2",
        "dotenv": "^16.4.7",
        "express": "^5.1.0",
        "intuit-oauth": "^4.0.0",
        "node-quickbooks": "^2.0.43",
        "open": "^9.1.0",
        "zod": "^3.24.2",
        "zod-to-json-schema": "^3.24.6"
    },
    "devDependencies": {
        "@eslint/js": "^9.22.0",
        "@types/express": "^5.0.4",
        "@types/node": "^22.13.10",
        "eslint": "^9.22.0",
        "eslint-config-prettier": "^10.1.1",
        "globals": "^16.0.0",
        "prettier": "3.5.3",
        "shx": "^0.3.4",
        "typescript": "^5.8.2",
        "typescript-eslint": "^8.26.1"
    }
}
